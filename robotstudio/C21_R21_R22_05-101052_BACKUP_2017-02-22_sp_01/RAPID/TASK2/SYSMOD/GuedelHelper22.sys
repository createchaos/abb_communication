    ENDPROC

    PROC rAAG()
        AccSet 30,30;
        MoveAbsJ [[-74.8101,3.64299,24.0372,221.102,-58.5283,-12.2541],[38454.3,-12049.9,-3981.15,0,0,0]]\NoEOffs,v100,fine,tool0;
