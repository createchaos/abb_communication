    PROC Routine1()
        AccSet 30,30;
        MoveAbsJ [[0,-65,65,0,0,0],[35000,-7750,-4500,0,0,0]]\NoEOffs,v1000,z50,tool0;
    ENDPROC

    PROC Routine2()
        AccSet 30,30;
